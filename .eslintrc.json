{
    "extends": ["@imaginary-cloud/react"],
    "plugins": ["react", "jsx-a11y", "react-redux", "react-extra", "react-hooks", "prettier", "autofix", "filenames"],
    "settings": {
        "import/resolver": {
            "node": {
                "extensions": [".js", ".jsx", ".ts", ".tsx"]
            }
        }
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true,
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "func-style": [0, "declaration", {"allowArrowFunctions": true}],
        "react/function-component-definition": 0,
        "sonarjs/cognitive-complexity": 0,
        "sonarjs/no-all-duplicated-branches": 0,
        "sonarjs/no-duplicate-string": 0,
        "unicorn/explicit-length-check": 0,
        "unicorn/no-abusive-eslint-disable": 0,
        "eslint-comments/no-unlimited-disable": 0,
        "no-shadow": 0,
        "react/jsx-newline": 0,
        "no-param-reassign": 0,
        "array-func/prefer-array-from": 0,
        "react/require-optimization": 0,
        "no-underscore-dangle": 0,
        "no-plusplus": 0,
        "multiline-comment-style": 0,
        "jsx-a11y/no-noninteractive-element-interactions": 0,
        "radix": 0,
        "no-warning-comments": 0,
        "react/no-array-index-key": 0,
        "scanjs-rules/assign_to_location": 0,
        "max-lines-per-function": 0,
        "react/jsx-max-depth": [
            2,
            {
                "max": 8
            }
        ],
        "no-invalid-this": 0,
        "react-extra/no-inline-styles": 0,
        "sort-vars": 0,
        "sort-keys": 0,
        "sort-imports": 0,
        "simple-import-sort/imports": 0,
        "simple-import-sort/exports": 0,
        "no-process-env": 0,
        "react/jsx-no-literals": 0,
        "react/jsx-uses-react": 2,
        "react/jsx-uses-vars": 2,
        "react/jsx-handler-names": 0,
        "id-length": 0,
        "react/no-set-state": 0,
        "no-ternary": 0,
        "react/jsx-no-bind": 0,
        "no-else-return": 2,
        "react/no-deprecated": 2,
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [".js", ".jsx"]
            }
        ],
        "prettier/prettier": [
            1,
            {
                "printWidth": 150,
                "tabWidth": 4,
                "useTabs": true,
                "bracketSpacing": false,
                "trailingComma": "all",
                "semi": true,
                "endOfLine": "auto",
                "singleQuote": false
            }
        ],
        "react-redux/connect-prefer-named-arguments": 2,
        "react/prop-types": 0,
        "max-len": [
            2,
            {
                "code": 150
            }
        ],
        "brace-style": [2, "1tbs"],
        "semi": [2, "always"],
        "object-curly-spacing": [2, "never"],
        "react/jsx-boolean-value": 0,
        "react/jsx-props-no-spreading": 0,
        "react-hooks/rules-of-hooks": 2,
        "react-hooks/exhaustive-deps": 1,
        "unicorn/prevent-abbreviations": 0,
        "unicorn/filename-case": 0,
        "unicorn/consistent-function-scoping": 0,
        "react-redux/prefer-separate-component-file": 0,
        "import/no-named-as-default": 0,
        "camelcase": 2,
        "react/state-in-constructor": 0,
        "react/no-multi-comp": [2, {"ignoreStateless": true}],
        "no-useless-constructor": 2,
        "comma-style": [2, "last"],
        "one-var": [
            2,
            {
                "let": "always",
                "var": "always",
                "const": "never"
            }
        ],
        "lodash/prefer-lodash-typecheck": 0,
        "jsx-a11y/label-has-associated-control": 0,
        "prefer-named-capture-group": 0,
        "require-unicode-regexp": 0,
        "import/extensions": 0,
        "optimize-regex/optimize-regex": 0,
        "sonarjs/no-small-switch": 0,
        "promise/no-promise-in-callback": 0,
        "promise/no-return-wrap": 0,
        "promise/catch-or-return": 0,
        "promise/always-return": 0,
        "lodash/prefer-get": 0,
        "default-param-last": 0,
        "init-declarations": 0,
        "no-magic-numbers": 0,
        "lodash/prefer-immutable-method": 0,
        "no-case-declarations": 0,
        "no-prototype-builtins": 0,
        "no-unused-vars": 1,
        "scanjs-rules/call_connect": 0,
        "import/prefer-default-export": 0,
        "react/jsx-indent": 0,
        "react/jsx-indent-props": 0,
        "indent": 0,
        "lodash/prefer-reject": 0,
        "default-case-last": 0,
        "no-loss-of-precision": 0,
        "no-useless-backreference": 0,
        "lodash-fp/no-unused-result": 0,
        "no-unreachable-loop": 0,
        "max-nested-callbacks": 0,
        "jsx-a11y/click-events-have-key-events": 0,
        "jsx-a11y/no-static-element-interactions": 0,
        "no-promise-executor-return": 0,
        "scanjs-rules/identifier_localStorage":0,
        "promise/no-nesting":0,
        "no-console": 2,
        "options": [0,{
            "failOnWarning": false,
            "failOnError": false
        }],
        "func-name-matching": 0,
        "consistent-return": 0,
        "import/no-unresolved": 0,
        "lodash-fp/no-chain":0,
        "scanjs-rules/property_localStorage":0,
        "lodash/no-unbound-this":0,
        "lodash/chaining":0,
        "lodash/prefer-find":0,
        "lodash/collection-ordering":0,
        "lodash-fp/prefer-constant":0,
        "react/jsx-no-constructed-context-values": 0
    }
}
